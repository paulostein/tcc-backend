{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "indent": "off",
        "max-len": ["error", 120],
        "new-cap": "off",
        "require-jsdoc": "off"
    }
}